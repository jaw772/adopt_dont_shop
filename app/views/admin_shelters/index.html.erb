<h1 style="text-decoration:underline;">Welcome to the Adminstration Page</h1><br><br>
<h2>Below you will find all of the shelters located within the system</h2>

<% @shelters.each do |shelter| %>
  <h3>
    <%= link_to "#{shelter.name}", "/shelters/#{shelter.id}" %><br>
    <% AdminShelter.applicants(shelter).each do |applicant|%>
      <span><%= link_to "#{applicant.name}", "/admin/applications/#{applicant.id}"%></span>
    <% end %>
  </h3>
<% end %>
